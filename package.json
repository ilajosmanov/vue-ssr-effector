{
  "name": "vue-effector-ssr",
  "version": "0.0.0",
  "private": "true",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "node ./server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-tsc --noEmit && vite build --outDir dist/client --ssrManifest",
    "build:server": "vue-tsc --noEmit && vite build --outDir dist/server --ssr src/entry-server.ts",
    "serve": "cross-env NODE_ENV=production node server",
    "lint": "eslint '*/**/*.{vue,js,ts}' --quiet --fix"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "effector": "^22.1.0",
    "express": "^4.17.1",
    "serve-static": "^1.14.1",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@logux/eslint-config": "^45.4.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/babel__core": "^7.1.16",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/server-renderer": "^3.2.9",
    "cross-env": "^7.0.3",
    "effector-logger": "^0.12.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "simple-git-hooks": "^2.6.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.5",
    "vue-eslint-parser": "^7.11.0",
    "vue-tsc": "^0.3.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts}": [
      "npm run lint",
      "git add"
    ]
  }
}
